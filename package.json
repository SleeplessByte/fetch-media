{
  "name": "fetch-media",
  "version": "2.0.0-rc1",
  "description": "Utility function that uses fetch to fetch a media-enabled resource",
  "main": "index.cjs.js",
  "module": "index.es.js",
  "types": "index.d.ts",
  "license": "MIT",
  "scripts": {
    "build": "yarn build:main && yarn build:module && yarn build:dts",
    "build:main": "rimraf dist && yarn ncc build index.ts -o dist --target es3 && move .\\dist\\index.js index.cjs.js",
    "build:module": "yarn tsc --module es2015 && move .\\index.js .\\index.es.js",
    "build:dts": "yarn tsc --declaration --declarationMap --emitDeclarationOnly",
    "prepare": "yarn build"
  },
  "peerDependencies": {
    "tslib": "^2.1.0"
  },
  "devDependencies": {
    "@babel/core": "^7.12.16",
    "@babel/preset-env": "^7.12.16",
    "@babel/preset-typescript": "^7.12.16",
    "@types/jest": "^26.0.20",
    "@typescript-eslint/eslint-plugin": "^4.15.1",
    "@typescript-eslint/parser": "^4.15.1",
    "@vercel/ncc": "^0.28.5",
    "babel-jest": "^26.6.3",
    "eslint": "^7.20.0",
    "eslint-config-universe": "^7.0.1",
    "jest": "^26.6.3",
    "prettier": "^2.2.1",
    "rimraf": "^3.0.2",
    "tslib": "^2.1.0",
    "typescript": "~4.1.5"
  },
  "dependencies": {
    "@ungap/url-search-params": "^0.2.2"
  }
}
