  "description": "Utility function that uses fetch to fetch a media-enabled resource",
  "main": "index.cjs.js",
  "module": "index.es.js",
  "types": "index.d.ts",
  "license": "MIT",
  "scripts": {
    "build": "yarn build:main && yarn build:module && yarn build:dts",
