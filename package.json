    "@typescript-eslint/eslint-plugin": "^4.15.1",
    "@typescript-eslint/parser": "^4.15.1",
    "babel-jest": "^26.6.3",
    "eslint": "^7.20.0",
    "eslint-config-universe": "^7.0.1",
    "jest": "^26.6.3",
    "prettier": "^2.2.1",
    "tslib": "^2.1.0",
    "typescript": "~4.1.5"
  },
  "files": [
    "index.cjs.js",
    "index.es.js",
    "index.ts",
